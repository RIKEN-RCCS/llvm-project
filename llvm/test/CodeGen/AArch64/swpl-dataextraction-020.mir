body:             |
  bb.0 (%ir-block.0):
    successors: %bb.2(0x80000000)
  
    %9:gpr64all = COPY $xzr
    %6:gpr64all = COPY %9
    %10:gpr32 = MOVi32imm 100
    %7:gpr64all = SUBREG_TO_REG 0, killed %10, %subreg.sub_32
    %11:gpr64common = MOVaddr target-flags(aarch64-page) @a, target-flags(aarch64-pageoff, aarch64-nc) @a
    %14:gpr64common = MOVaddr target-flags(aarch64-page) @c, target-flags(aarch64-pageoff, aarch64-nc) @c
    B %bb.2
  
  bb.1 (%ir-block.2):
    %3:gpr64 = COPY %12
    %19:gpr64common = ADRP target-flags(aarch64-page) @x
    STRXui %3, killed %19, target-flags(aarch64-pageoff, aarch64-nc) @x :: (store 8 into `i64* bitcast (double* @x to i64*)`, !tbaa !2)
    RET_ReallyLR
  
  bb.2 (%ir-block.3):
    successors: %bb.2(0x7c000000), %bb.1(0x04000000)
  
    %1:gpr64common = PHI %6, %bb.0, %4, %bb.2
    %2:gpr64sp = PHI %7, %bb.0, %5, %bb.2
    %12:gpr64 = LDRXroX %11, %1, 0, 0 :: (load 8 from %ir.uglygep46, !tbaa !2)
    %13:fpr64 = COPY %12
    %15:fpr64 = LDRDroX %14, %1, 0, 0 :: (load 8 from %ir.uglygep1, !tbaa !2)
    %16:fpr64 = FMULDrr killed %15, killed %13
    %20:fpr64 = COPY %16
    STRDroX killed %16, %11, %1, 0, 0 :: (store 8 into %ir.uglygep45, !tbaa !2)
    STRDroX killed %20, %11, %1, 0, 0 :: (store 8 into %ir.uglygep45, !tbaa !2)
    %17:gpr64sp = nuw nsw ADDXri %1, 8, 0
    %4:gpr64all = COPY %17
    %18:gpr64 = SUBSXri %2, 1, 0, implicit-def $nzcv
    %5:gpr64all = COPY %18
    Bcc 1, %bb.2, implicit $nzcv
    B %bb.1

...
