# RUN: llc -swpl-debug -mcpu=a64fx -O1 -fswp -start-before=aarch64-swpipeliner %s -o /dev/null 2>&1 | FileCheck %s
#
# CHECK: DBG(SwplLoop::convertPrePostIndexInstr)
# CHECK-NEXT:  before:early-clobber %52:gpr64sp, %53:fpr64 = LDRDpre %50:gpr64sp(tied-def 0), -8 :: (load (s64) from %ir.uglygep30, !tbaa !9)
# CHECK-NEXT:  after 1:%53:fpr64 = LDURDi %50:gpr64sp, -8 :: (load (s64) from %ir.uglygep30, !tbaa !9)
# CHECK-NEXT:  after 2:%52:gpr64sp = SUBXri %50:gpr64sp, 8, 0

--- |
  ; ModuleID = 's112.c'
  source_filename = "s112.c"
  target datalayout = "e-m:e-i8:8:32-i16:16:32-i64:64-i128:128-n32:64-S128"
  target triple = "aarch64-unknown-linux-gnu"
  
  %struct.args_t = type { %struct.timeval, %struct.timeval, ptr }
  %struct.timeval = type { i64, i64 }
  
  @__func__.s112 = private unnamed_addr constant [5 x i8] c"s112\00", align 1
  @a = external global [32000 x double], align 64
  @b = external global [32000 x double], align 64
  @c = external global [32000 x double], align 64
  @d = external global [32000 x double], align 64
  @e = external global [32000 x double], align 64
  @aa = external global [256 x [256 x double]], align 64
  @bb = external global [256 x [256 x double]], align 64
  @cc = external global [256 x [256 x double]], align 64
  
  ; Function Attrs: nounwind uwtable vscale_range(4,4)
  define dso_local double @s112(ptr nocapture noundef %func_args) local_unnamed_addr #0 {
  entry:
    %call = tail call i32 @initialise_arrays(ptr noundef nonnull @__func__.s112) #4
    %call1 = tail call i32 @gettimeofday(ptr noundef %func_args, ptr noundef null) #4
    br label %for.cond2.preheader
  
  for.cond2.preheader:                              ; preds = %entry, %for.cond.cleanup4
    %nl.023 = phi i32 [ 0, %entry ], [ %inc, %for.cond.cleanup4 ]
    %0 = call i64 @llvm.start.loop.iterations.i64(i64 31999)
    br label %for.body5
  
  for.cond.cleanup:                                 ; preds = %for.cond.cleanup4
    %t2 = getelementptr inbounds %struct.args_t, ptr %func_args, i64 0, i32 1
    %call14 = tail call i32 @gettimeofday(ptr noundef nonnull %t2, ptr noundef null) #4
    %call15 = tail call fast double @calc_checksum(ptr noundef nonnull @__func__.s112) #4
    ret double %call15
  
  for.cond.cleanup4:                                ; preds = %for.body5
    %call11 = tail call i32 @dummy(ptr noundef nonnull @a, ptr noundef nonnull @b, ptr noundef nonnull @c, ptr noundef nonnull @d, ptr noundef nonnull @e, ptr noundef nonnull @aa, ptr noundef nonnull @bb, ptr noundef nonnull @cc, double noundef 0.000000e+00) #4
    %inc = add nuw nsw i32 %nl.023, 1
    %exitcond.not = icmp eq i32 %inc, 30
    br i1 %exitcond.not, label %for.cond.cleanup, label %for.cond2.preheader, !llvm.loop !6
  
  for.body5:                                        ; preds = %for.cond2.preheader, %for.body5
    %lsr.iv28 = phi i64 [ 255984, %for.cond2.preheader ], [ %lsr.iv.next, %for.body5 ]
    %lsr.iv = phi ptr [ getelementptr (i8, ptr @a, i64 255992), %for.cond2.preheader ], [ %uglygep27, %for.body5 ]
    %1 = phi i64 [ %0, %for.cond2.preheader ], [ %4, %for.body5 ]
    %uglygep30 = getelementptr i8, ptr %lsr.iv, i64 -8
    %2 = load double, ptr %uglygep30, align 8, !tbaa !9
    %uglygep29 = getelementptr i8, ptr @b, i64 %lsr.iv28
    %3 = load double, ptr %uglygep29, align 8, !tbaa !9
    %add = fadd fast double %3, %2
    store double %add, ptr %lsr.iv, align 8, !tbaa !9
    %uglygep27 = getelementptr i8, ptr %lsr.iv, i64 -8
    %lsr.iv.next = add nsw i64 %lsr.iv28, -8
    %4 = call i64 @llvm.loop.decrement.reg.i64(i64 %1, i64 1)
    %5 = icmp ne i64 %4, 0
    br i1 %5, label %for.body5, label %for.cond.cleanup4, !llvm.loop !13
  }
  
  declare i32 @initialise_arrays(ptr noundef) local_unnamed_addr #1
  
  ; Function Attrs: nofree nounwind
  declare noundef i32 @gettimeofday(ptr nocapture noundef, ptr nocapture noundef) local_unnamed_addr #2
  
  declare i32 @dummy(ptr noundef, ptr noundef, ptr noundef, ptr noundef, ptr noundef, ptr noundef, ptr noundef, ptr noundef, double noundef) local_unnamed_addr #1
  
  declare double @calc_checksum(ptr noundef) local_unnamed_addr #1
  
  ; Function Attrs: nocallback noduplicate nofree nosync nounwind willreturn
  declare i64 @llvm.start.loop.iterations.i64(i64) #3
  
  ; Function Attrs: nocallback noduplicate nofree nosync nounwind willreturn
  declare i64 @llvm.loop.decrement.reg.i64(i64, i64) #3
  
  attributes #0 = { nounwind uwtable vscale_range(4,4) "approx-func-fp-math"="true" "frame-pointer"="non-leaf" "min-legal-vector-width"="0" "no-infs-fp-math"="true" "no-nans-fp-math"="true" "no-signed-zeros-fp-math"="true" "no-trapping-math"="true" "stack-protector-buffer-size"="8" "target-cpu"="a64fx" "target-features"="+aes,+crc,+crypto,+fp-armv8,+fullfp16,+lse,+neon,+outline-atomics,+ras,+rdm,+sha2,+sve,+v8.2a" "unsafe-fp-math"="true" }
  attributes #1 = { "approx-func-fp-math"="true" "frame-pointer"="non-leaf" "no-infs-fp-math"="true" "no-nans-fp-math"="true" "no-signed-zeros-fp-math"="true" "no-trapping-math"="true" "stack-protector-buffer-size"="8" "target-cpu"="a64fx" "target-features"="+aes,+crc,+crypto,+fp-armv8,+fullfp16,+lse,+neon,+outline-atomics,+ras,+rdm,+sha2,+sve,+v8.2a" "unsafe-fp-math"="true" }
  attributes #2 = { nofree nounwind "approx-func-fp-math"="true" "frame-pointer"="non-leaf" "no-infs-fp-math"="true" "no-nans-fp-math"="true" "no-signed-zeros-fp-math"="true" "no-trapping-math"="true" "stack-protector-buffer-size"="8" "target-cpu"="a64fx" "target-features"="+aes,+crc,+crypto,+fp-armv8,+fullfp16,+lse,+neon,+outline-atomics,+ras,+rdm,+sha2,+sve,+v8.2a" "unsafe-fp-math"="true" }
  attributes #3 = { nocallback noduplicate nofree nosync nounwind willreturn }
  attributes #4 = { nounwind }
  
  !llvm.module.flags = !{!0, !1, !2, !3, !4}
  !llvm.ident = !{!5}
  
  !0 = !{i32 1, !"wchar_size", i32 4}
  !1 = !{i32 7, !"PIC Level", i32 2}
  !2 = !{i32 7, !"PIE Level", i32 2}
  !3 = !{i32 7, !"uwtable", i32 2}
  !4 = !{i32 7, !"frame-pointer", i32 1}
  !5 = !{!"clang version 15.0.4 ()"}
  !6 = distinct !{!6, !7, !8}
  !7 = !{!"llvm.loop.mustprogress"}
  !8 = !{!"llvm.loop.unroll.disable"}
  !9 = !{!10, !10, i64 0}
  !10 = !{!"double", !11, i64 0}
  !11 = !{!"omnipotent char", !12, i64 0}
  !12 = !{!"Simple C/C++ TBAA"}
  !13 = distinct !{!13, !7, !8}

...
