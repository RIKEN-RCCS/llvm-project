  define dso_local void @foo() local_unnamed_addr #0 {
    %1 = load i32, i32* @y, align 4, !tbaa !2
    call void @llvm.set.loop.iterations.i64(i64 100)
    br label %3
  
  2:                                                ; preds = %3
    ret void
  
  3:                                                ; preds = %3, %0
    %lsr.iv = phi i64 [ %lsr.iv.next, %3 ], [ 0, %0 ]
    %4 = phi i64 [ 100, %0 ], [ %10, %3 ]
    %uglygep = getelementptr i8, i8* bitcast (i32* getelementptr ([1000 x i32], [1000 x i32]* @a, i64 0, i64 -1) to i8*), i64 %lsr.iv
    %uglygep1 = bitcast i8* %uglygep to i32*
    %5 = load i32, i32* %uglygep1, align 4, !tbaa !2
    %uglygep4 = getelementptr i8, i8* bitcast ([1000 x i32]* @c to i8*), i64 %lsr.iv
    %uglygep45 = bitcast i8* %uglygep4 to i32*
    %6 = load i32, i32* %uglygep45, align 4, !tbaa !2
    %7 = sub nsw i32 %5, %6
    %uglygep9 = getelementptr i8, i8* bitcast ([1000 x i32]* @b to i8*), i64 %lsr.iv
    %uglygep910 = bitcast i8* %uglygep9 to i32*
    store i32 %7, i32* %uglygep910, align 4, !tbaa !2
    %uglygep6 = getelementptr i8, i8* bitcast ([1000 x i32]* @c to i8*), i64 %lsr.iv
    %uglygep67 = bitcast i8* %uglygep6 to i32*
    %scevgep8 = getelementptr i32, i32* %uglygep67, i64 1
    %8 = load i32, i32* %scevgep8, align 4, !tbaa !2
    %9 = mul nsw i32 %8, %1
    %uglygep2 = getelementptr i8, i8* bitcast (i32* getelementptr ([1000 x i32], [1000 x i32]* @a, i64 0, i64 -1) to i8*), i64 %lsr.iv
    %uglygep23 = bitcast i8* %uglygep2 to i32*
    %scevgep = getelementptr i32, i32* %uglygep23, i64 3
    store i32 %9, i32* %scevgep, align 4, !tbaa !2
    %lsr.iv.next = add nuw nsw i64 %lsr.iv, 4
    %10 = call i64 @llvm.loop.decrement.reg.i64(i64 %4, i64 1)
    %11 = icmp ne i64 %10, 0
    br i1 %11, label %3, label %2, !llvm.loop !6
  }
  
