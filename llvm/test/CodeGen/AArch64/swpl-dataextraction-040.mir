    %0:gpr64common = PHI %4, %bb.0, %2, %bb.2
    %1:gpr64sp = PHI %5, %bb.0, %3, %bb.2
    %9:fpr64 = LDRDroX %8, %0, 0, 0 :: (load 8 from %ir.uglygep45, !tbaa !2)
    %11:fpr64 = LDRDroX %10, %0, 0, 0 :: (load 8 from %ir.uglygep23, !tbaa !2)
    %12:fpr64 = FADDDrr killed %9, killed %11
    STRDroX killed %12, %13, %0, 0, 0 :: (store 8 into %ir.uglygep1, !tbaa !2)
    %14:gpr64sp = nuw nsw ADDXri %0, 8, 0
    %2:gpr64all = COPY %14
    %15:gpr64 = SUBSXri %1, 1, 0, implicit-def $nzcv
    %3:gpr64all = COPY %15
    Bcc 1, %bb.2, implicit $nzcv
    B %bb.1

