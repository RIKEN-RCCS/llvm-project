  ; ModuleID = '2937-33.ll'
  source_filename = "2937-33.c"
  target datalayout = "e-m:e-i8:8:32-i16:16:32-i64:64-i128:128-n32:64-S128"
  target triple = "aarch64-unknown-linux"
  
  @b = dso_local local_unnamed_addr global [1000 x i32] zeroinitializer, align 4
  @x = dso_local local_unnamed_addr global i32 0, align 4
  @a = dso_local local_unnamed_addr global [1000 x i32] zeroinitializer, align 4
  @y = dso_local local_unnamed_addr global i32 0, align 4
  @c = dso_local local_unnamed_addr global [1000 x i32] zeroinitializer, align 4
  
  ; Function Attrs: nofree norecurse nounwind
  define dso_local void @foo() local_unnamed_addr #0 {
    %1 = load i32, i32* @x, align 4, !tbaa !2
    %2 = load i32, i32* @y, align 4, !tbaa !2
    call void @llvm.set.loop.iterations.i64(i64 100)
    br label %4
  
  3:                                                ; preds = %4
    ret void
  
  4:                                                ; preds = %4, %0
    %lsr.iv = phi i64 [ %lsr.iv.next, %4 ], [ 0, %0 ]
    %5 = phi i64 [ 100, %0 ], [ %10, %4 ]
    %uglygep6 = getelementptr i8, i8* bitcast ([1000 x i32]* @b to i8*), i64 %lsr.iv
    %uglygep67 = bitcast i8* %uglygep6 to i32*
    %6 = load i32, i32* %uglygep67, align 4, !tbaa !2
    %7 = mul nsw i32 %1, %6
    %uglygep4 = getelementptr i8, i8* bitcast ([1000 x i32]* @a to i8*), i64 %lsr.iv
    %uglygep45 = bitcast i8* %uglygep4 to i32*
    store i32 %7, i32* %uglygep45, align 4, !tbaa !2
    %uglygep2 = getelementptr i8, i8* bitcast (i32* getelementptr inbounds ([1000 x i32], [1000 x i32]* @c, i64 0, i64 1) to i8*), i64 %lsr.iv
    %uglygep23 = bitcast i8* %uglygep2 to i32*
    %8 = load i32, i32* %uglygep23, align 4, !tbaa !2
    %9 = mul nsw i32 %8, %2
    %uglygep = getelementptr i8, i8* bitcast ([1000 x i32]* @a to i8*), i64 %lsr.iv
    %uglygep1 = bitcast i8* %uglygep to i32*
    %scevgep = getelementptr i32, i32* %uglygep1, i64 1
    store i32 %9, i32* %scevgep, align 4, !tbaa !2
    %lsr.iv.next = add nuw nsw i64 %lsr.iv, 4
    %10 = call i64 @llvm.loop.decrement.reg.i64(i64 %5, i64 1)
    %11 = icmp ne i64 %10, 0
    br i1 %11, label %4, label %3, !llvm.loop !6
  }
  
  ; Function Attrs: noduplicate nounwind
  declare void @llvm.set.loop.iterations.i64(i64) #1
  
  ; Function Attrs: noduplicate nounwind
  declare i64 @llvm.loop.decrement.reg.i64(i64, i64) #1
  
  ; Function Attrs: nounwind
  declare void @llvm.stackprotector(i8*, i8**) #2
  
  attributes #0 = { nofree norecurse nounwind "correctly-rounded-divide-sqrt-fp-math"="false" "disable-tail-calls"="false" "frame-pointer"="non-leaf" "less-precise-fpmad"="false" "min-legal-vector-width"="0" "no-infs-fp-math"="false" "no-jump-tables"="false" "no-nans-fp-math"="false" "no-signed-zeros-fp-math"="false" "no-trapping-math"="true" "stack-protector-buffer-size"="8" "target-cpu"="a64fx" "target-features"="+aes,+crc,+crypto,+fp-armv8,+fullfp16,+lse,+neon,+ras,+rdm,+sha2,+sve,+v8.2a" "unsafe-fp-math"="false" "use-soft-float"="false" }
  attributes #1 = { noduplicate nounwind }
