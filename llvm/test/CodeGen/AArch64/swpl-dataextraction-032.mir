    %1:gpr64sp = PHI %5, %bb.0, %3, %bb.2
    %9:gpr32 = LDRWroX %8, %0, 0, 0 :: (load 4 from %ir.uglygep67, !tbaa !2)
    %11:gpr64common = ADDXrr %10, %0
    STRWui killed %9, %11, 0 :: (store 4 into %ir.uglygep45, !tbaa !2)
    %12:gpr32 = LDRWui %11, 1 :: (load 4 from %ir.scevgep, !tbaa !2)
    STRWroX killed %12, %13, %0, 0, 0 :: (store 4 into %ir.uglygep1, !tbaa !2)
    %14:gpr64sp = nuw nsw ADDXri %0, 4, 0
    %2:gpr64all = COPY %14
    %15:gpr64 = SUBSXri %1, 1, 0, implicit-def $nzcv
    %3:gpr64all = COPY %15
    Bcc 1, %bb.2, implicit $nzcv
    B %bb.1

