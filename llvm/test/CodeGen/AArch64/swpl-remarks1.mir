    %3:fpr64 = PHI %17, %bb.1, %5, %bb.2
    %4:gpr64sp = PHI %1, %bb.1, %7, %bb.2
    %22:zpr = LD1D %21, %20, %2 :: (load unknown-size from %ir.uglygep, align 64, !tbaa !6)
    %23:zpr = INSERT_SUBREG %24, %3, %subreg.dsub
    %26:zpr = FADDA_VPZ_D %21, %23, killed %22
    %5:fpr64 = COPY %26.dsub
    %27:gpr64sp = nuw ADDXri %2, 8, 0
    %6:gpr64all = COPY %27
    %28:gpr64 = SUBSXri %4, 1, 0, implicit-def $nzcv
    %7:gpr64all = COPY %28
    Bcc 1, %bb.2, implicit $nzcv
    B %bb.4
  
  bb.3.for.cond.cleanup:
    %40:gpr64sp = nuw ADDXri %9, 16, 0
    ADJCALLSTACKDOWN 0, 0, implicit-def dead $sp, implicit $sp
    %41:gpr64all = COPY $xzr
    $x0 = COPY %40
    $x1 = COPY %41
    BL @gettimeofday, csr_aarch64_aapcs, implicit-def dead $lr, implicit $sp, implicit $x0, implicit $x1, implicit-def $sp, implicit-def $w0
    ADJCALLSTACKUP 0, 0, implicit-def dead $sp, implicit $sp
    %43:gpr64common = MOVaddr target-flags(aarch64-page) @__func__.s311, target-flags(aarch64-pageoff, aarch64-nc) @__func__.s311
    $x0 = COPY %43
    TCRETURNdi @calc_checksum, 0, csr_aarch64_aapcs, implicit $sp, implicit $x0
  
  bb.4.for.cond.cleanup4:
    successors: %bb.3(0x04000000), %bb.1(0x7c000000)
  
    ADJCALLSTACKDOWN 0, 0, implicit-def dead $sp, implicit $sp
    $x0 = COPY %20
    $x1 = COPY %30
    $x2 = COPY %31
    $x3 = COPY %32
    $x4 = COPY %33
    $x5 = COPY %34
    $x6 = COPY %35
    $x7 = COPY %36
    $d0 = COPY %5
    BL @dummy, csr_aarch64_aapcs, implicit-def dead $lr, implicit $sp, implicit $x0, implicit $x1, implicit $x2, implicit $x3, implicit $x4, implicit $x5, implicit $x6, implicit $x7, implicit $d0, implicit-def $sp, implicit-def $w0
    ADJCALLSTACKUP 0, 0, implicit-def dead $sp, implicit $sp
    %38:gpr32sp = nuw nsw ADDWri %0, 1, 0
    %8:gpr32all = COPY %38
    dead $wzr = SUBSWri %38, 100, 0, implicit-def $nzcv
    Bcc 0, %bb.3, implicit $nzcv
    B %bb.1

