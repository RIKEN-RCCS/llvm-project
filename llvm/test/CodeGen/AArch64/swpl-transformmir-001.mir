  ; Function Attrs: norecurse nounwind readonly
  define dso_local i64 @test3044(i64 %0) local_unnamed_addr #0 {
    %2 = icmp slt i64 %0, 0
    br i1 %2, label %4, label %.preheader
  
  .preheader:                                       ; preds = %1
    %3 = add i64 %0, 1
    call void @llvm.set.loop.iterations.i64(i64 %3)
    br label %6
  
