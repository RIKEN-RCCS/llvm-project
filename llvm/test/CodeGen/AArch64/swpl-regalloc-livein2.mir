# CHECK-NEXT:  SWPLIVEIN implicit-def $z0, implicit-def $z1, implicit-def $w2, implicit-def $w3, implicit-def $p1, implicit-def $x8
# CHECK-NEXT:  %45:zpr = COPY $z0
# CHECK-NEXT:  %46:zpr = COPY $z1
